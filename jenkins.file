pipeline{
    agent any
    stages{
        stage("clone"){
            steps{
                bat "git clone https://github.com/patilrupashree/jenkins-demo2.git"
            }
        }
        stage("compile"){
            steps{
            bat "C:/Maven zip/apache-maven-3.8.4/bin/mvn clean compile"
            }
        }
        
         stage("package"){
            steps{
            bat "C:/Maven zip/apache-maven-3.8.4/bin/mvn package"
            }
        }
       
      }
   }
